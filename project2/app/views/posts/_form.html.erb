<%= form_for @post do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :content %>
  <%= f.text_field :content %>

  <%= f.label :category %>
  <%= f.select(:category, options_for_select(['Camping', 'Backpacking', 'Dispersed Camping', 'Trails'])) %>
  <!-- <%= f.text_field :category %> -->

  <%= f.submit %>

<% end %>
